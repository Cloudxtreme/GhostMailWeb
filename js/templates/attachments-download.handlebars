<tr class="template-download fade in enabled" data-fileid="{{uuid}}" data-size={{bytes}} title="Click to unlock and download this attachment!" style="cursor: pointer;">
    <td class="name" width="30%"><span class="filename">{{name}}</span></td>
    <td class="size" width="10%"><span>{{size}}</span></td>
    <td class="error" width="50%" colspan="2">
        <div class="progress" style="display: none;">
            <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%">                
            </div>
        </div>
        <span class="progress-percent pull-right" style="display: none;">0%</span>
    </td>
    <td class="delete" width="10%" align="right">
        
    </td>
</tr>