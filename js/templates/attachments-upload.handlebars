<tr class="template-download fade in" data-fileid="{{id}}" data-uuid="" data-size={{bytes}}>
    <td class="name" width="30%"><span><a href="{{url}}" download="{{name}}">{{name}}</a></span></td>
    <td class="size" width="10%"><span>{{size}}</span></td>
    <td class="error" width="50%" colspan="2">
        <div class="progress">
            <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%">
                
            </div>
        </div>
        <span class="progress-percent pull-right">0%</span>
    </td>
    <td class="delete" width="10%" align="right">        
        <button style="display: none;" class="btn default btn-sm red-sunglo" data-type="" data-url="">
            <i class="fa fa-times"></i>
        </button>
    </td>
</tr>