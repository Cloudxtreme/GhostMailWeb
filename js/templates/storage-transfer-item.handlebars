<tr class="template-download fade in" data-item-id="{{id}}" data-item-total-size={{total_size}} data-item-size={{bytes}} data-item-partsize={{part_size}} data-item-bytes-transfered="0">
    <td class="name" width="30%"><span><i class="{{type}} fa {{icon}}"></i> {{name}}</span></td>
    <td class="size" width="10%"><span>{{size}}</span></td>
    <td class="size" width="10%"><span>{{#if is_upload}}<i class="fa fa-cloud-upload"></i> Upload{{else}}<i class="fa fa-cloud-download"></i> Download{{/if}}</span></td>
    <td class="error" width="35%">
        <div class="progress">
            <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%">
                
            </div>
        </div>
        <span class="progress-percent pull-right">0%</span>
    </td>
    <td class="size" width="5%">&nbsp;</td>
</tr>